<ExtensionModel>

	<!-- Extensions -->
	
	<Extension path = "/MonoDevelop/Ide/Commands">
		<Command id = "MonoDevelop.WakaTime.Commands.ShowSettingsWindow"
		         defaultHandler = "MonoDevelop.WakaTime.ShowSettingsWindowHandler"
		         _label = "WakaTime Settings" />
	</Extension>

	<Extension path = "/MonoDevelop/Ide/MainMenu/Tools">
		<CommandItem id = "MonoDevelop.WakaTime.Commands.ShowSettingsWindow" insertbefore="OptionsSection" />
	</Extension>

	<Extension path = "/MonoDevelop/Ide/StartupHandlers">
	  <Class class = "MonoDevelop.WakaTime.ShowSettingsOnStartUpHandler"/>
	</Extension>

</ExtensionModel>
